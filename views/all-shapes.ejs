<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/styles.css" />

    <title>UFO SIGHTINGS</title>
  </head>
  <body>
    <nav class="navbar bg-dark fixed-top text-h1">
      <a class="m-auto" href="/sighting-report">Report</a>
      <a class="m-auto" href="/">Sightings</a>
      <a class="m-auto" href="/shapes">Shapes</a>
    </nav>
    <div
      style="height: 100vh"
      class="d-flex flex-row align-items-center justify-content-center"
    >
      <div class="container text-center text-light bg-dark">
        Shapes List
        <div class="row mt-5">
    
          <ul>
            <% let shapesArray = [] %>
          <% for(let i=0;i<sightingInfo.length;i++) {%>
            <% shapesArray.push(sightingInfo[i].shape) %>
            <% let uniqueShapes = [... new Set(shapesArray)] %>
            <li><a href="/shapes/<%=uniqueShapes[i]%>"><%= uniqueShapes[i]%></a> </li> 
            <% } %>

          </ul>
        </div>
      </div>
    </div>
  </body>
  <script
    src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"
  ></script>
</html>
